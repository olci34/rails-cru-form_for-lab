<h1>Edit <%=@song.name%></h1>

<%= form_for @song do |f| %>
    <%= f.label "Song Name:" %><br>
    <%= f.text_field :name %><br><br>
    <%= f.label "Song Artist ID:" %><br>
    <%= f.text_field :artist_id %><br><br>
    <%= f.label "Song Genre ID:" %><br>
    <%= f.text_field :genre_id %><br><br>
    <%= f.submit "Update Song" %>
<% end %>